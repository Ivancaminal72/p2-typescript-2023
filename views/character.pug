extends characters.pug 

block sel_character
    h1(class=characters[id].status)=characters[id].name
    div
        h2=characters[id].species
        if characters[id].gender == "Male"
            img(class="gender" src="data:image/png;base64,", alt="")
        else if characters[id].gender == "Female"
            img(class="gender" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQpVlnt3FgoQ89_-yF8z_lXjtGs-s-yksIYaQ&usqp=CAU", alt="")
        div
    img(src=characters[id].image, alt="")
        
